<odoo>
	<data>

        <record model="ir.ui.view" id="wk_field_values_view_form">
			<field name="name">wk.field.values.form</field>
			<field name="model">wk.field.values</field>
			<field name="arch" type="xml">
				<form>
					<sheet>
                        <group>
                            <group>
                                <field name="field_type" readonly="1"/>
                                <field name="field_value" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

		<record model="ir.ui.view" id="wk_field_values_view_list">
			<field name="name">wk.field.values.view.list</field>
			<field name="model">wk.field.values</field>
			<field name="arch" type="xml">
				<list editable="bottom">
					<field name="sequence" widget="handle" />
					<field name="field_value" />
				</list>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_wk_field_values_property_available">
			<field name="name">Is This Property Available</field>
			<field name="res_model">wk.field.values</field>
			<field name="view_mode">list,form</field>
            <field name="domain">[('field_type','=','is_this_property_available')]</field>
            <field name="view_id" ref="wk_franchisee_management.wk_field_values_view_list" />
            <field name="context">{'default_field_type': 'is_this_property_available'}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records found. Let's create one!
                </p>
            </field>
		</record>

